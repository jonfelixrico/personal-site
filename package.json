{
  "name": "portfolio",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore \"**/*.{ts,tsx,json,css,scss}\"",
    "format:fix": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,json,css,scss}\"",
    "docker:build": "docker build -t local/portfolio .",
    "docker:run": "docker rm /portfolio && docker run -p 3000:3000 --name portfolio local/portfolio"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.2.72",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.14",
    "classnames": "^2.5.1",
    "eslint": "8.37.0",
    "eslint-config-next": "14.1.4",
    "markdown-to-jsx": "^7.4.5",
    "next": "14.1.4",
    "postcss": "8.4.21",
    "rc-tooltip": "^6.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-resize-detector": "^9.1.1",
    "react-use": "^17.5.0",
    "sharp": "^0.32.6",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "sass": "^1.72.0"
  }
