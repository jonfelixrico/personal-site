    "markdown-to-jsx": "^7.4.5",
    "next": "14.1.4",
    "postcss": "8.4.38",
    "rc-tooltip": "^6.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-resize-detector": "^10.0.1",
